{
  "name": "bsharisemoore2017",
  "version": "1.0.0",
  "description": "bsharisemoore site 2017",
  "main": "index.html",
  "scripts": {
    "build-css": "stylus -w styl/master.styl --out public/css/style.min.css --compress",
    "build-js": "browserify js/**.js>public/js/script.min.js",
    "build": "npm run build-css",
    "server": "browser-sync start -s 'public/' -f 'public/index.html, js/*.js, public/css/*.css'",
    "start": "npm run server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JFarand/piscesrevolt.git"
  },
  "keywords": [
    "colorful",
    "bright",
    "typography",
    "animation",
    "cssgrid"
  ],
  "author": "owlasylum",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JFarand/piscesrevolt/issues"
  },
  "homepage": "https://github.com/JFarand/piscesrevolt#readme",
  "dependencies": {
    "gsap": "^1.19.1",
    "jquery": "^3.2.1"
  }
}
